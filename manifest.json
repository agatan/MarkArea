{
  "manifest_version": 2,
  "name": "markarea",
  "version": "0.0.1",
  "icons": {
    "16": "images/markarea_16.png",
    "48": "images/markarea_48.png",
    "128": "images/markarea_128.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/index.js"]
  }],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ]
}
